{
  "name": "tabletop-of-many-things-server",
  "version": "1.0.0",
  "description": "",
  "type": "commonjs",
  "main": "src/index.ts",
  "scripts": {
    "build": "tsc --watch -p tsconfig.json",
    "start": "nodemon dist/index.js",
    "dev": "run-p build start",
    "docker:setup": "rm -rf ./.postgres-data && docker-compose up",
    "db:init": "supabase init",
    "db:start": "supabase start",
    "db:stop": "supabase stop",
    "db:link": "supabase link",
    "db:login": "supabase login",
    "db:pull": "supabase db pull",
    "db:push": "supabase db push",
    "db:status": "supabase status",
    "db:sync-reset": "supabase db pull && supabase db dump --data-only --file supabase/seed.sql && supabase db reset",
    "db:sync": "supabase db dump --data-only --file supabase/seed.sql",
    "db:sync-live": "supabase db dump --data-only --file supabase/seed.sql --local && supabase db reset --linked",
    "db:reset": "supabase db reset"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "connect-pg-simple": "^10.0.0",
    "cors": "^2.8.5",
    "dotenv": "^16.4.4",
    "express": "^4.18.2",
    "express-session": "^1.17.3",
    "fs": "^0.0.1-security",
    "helmet": "^7.0.0",
    "npm-run-all": "^4.1.5",
    "passport": "^0.6.0",
    "passport-local": "^1.0.0",
    "pg": "^8.11.3",
    "socket.io": "^4.8.0",
    "supabase": "^1.172.2"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/connect-pg-simple": "^7.0.3",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/express-session": "^1.18.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "^20.5.9",
    "@types/passport-local": "^1.0.38",
    "@types/pg": "^8.11.6",
    "chai": "^4.3.8",
    "chai-http": "^4.4.0",
    "concurrently": "^8.2.2",
    "mocha": "^10.2.0",
    "nodemon": "^3.1.0",
    "typescript": "^5.5.4"
  }
}
